Program Example2; (* Factorial *)
   
Var n : Integer;
    i : Integer;

Function F(n : Integer) : Integer;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);
  End;

Begin
  i:=0;
  For n := 1 To 7 Do
    Begin
      i/=1;
      Call WriteLn;
      Call WriteI( F(i));
    End;
End. (* Factorial *)
